<h1><%= @project.name %></h1>
<div class='row'>
  <div class = 'col-md-6'>
    <h2>Projectinformations</h2>
    <ul>
      <% @project.attributes.each do |key, value| %>
        <li><%=key%> = <%=value%></li>
      <%end%>
    </ul>
    <div class = 'col-md-6 centered'>
      <%= link_to "Delete", @project, method: :delete,
            data: {confirm:"You sure?"},
            class:"btn btn-default" %>
      <%= link_to "Edit", edit_project_path(@project), class:"btn btn-default" %>
    </div>
  </div>
  <div class= 'col-md-6'>
    <h2>Milestones</h2>
  </div>
</div>
